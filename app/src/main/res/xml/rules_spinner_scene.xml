<?xml version="1.0" encoding="utf-8"?>
<MotionScene
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">

  <ConstraintSet android:id="@+id/start">
      <Constraint android:id="@+id/imageView7"
          android:layout_height="118dp"
          android:layout_width="328dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent" />
      <Constraint
          android:id="@+id/textView"
          app:layout_constraintEnd_toEndOf="@+id/imageView7"
          android:layout_width="259dp"
          android:layout_height="44dp"
          app:layout_constraintBottom_toBottomOf="@+id/imageView7"
          app:layout_constraintTop_toTopOf="@+id/imageView7"
          app:layout_constraintStart_toStartOf="@+id/imageView7" />
  </ConstraintSet>

  <ConstraintSet android:id="@+id/finshSpin">
      <Constraint android:id="@id/imageView7"/>
      <Constraint
          android:id="@+id/textView"
          app:layout_constraintEnd_toEndOf="@+id/imageView7"
          android:layout_width="259dp"
          android:layout_height="44dp"
          app:layout_constraintBottom_toBottomOf="@+id/imageView7"
          app:layout_constraintTop_toTopOf="@+id/imageView7"
          app:layout_constraintStart_toStartOf="@+id/imageView7" />
  </ConstraintSet>


    <ConstraintSet android:id="@+id/spintop" >
        <Constraint
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="328dp"
            android:layout_height="118dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView7"
            android:layout_marginTop="-120dp" />
        <Constraint
            android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            android:layout_width="259dp"
            android:layout_height="44dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7"
            app:layout_constraintStart_toStartOf="@+id/imageView7" />
    </ConstraintSet>
    <Transition
        app:constraintSetStart="@+id/start"
        app:constraintSetEnd="@+id/spintop"
        app:duration="538">

        <OnClick app:clickAction="transitionToEnd" />
        <OnSwipe app:maxAcceleration="20.33" />
    </Transition>
    <ConstraintSet android:id="@+id/spinbottom" >
        <Constraint
            android:layout_height="118dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="328dp"
            android:id="@+id/imageView7"
            android:layout_marginBottom="-120dp" />
        <Constraint
            android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            android:layout_width="259dp"
            android:layout_height="44dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7"
            app:layout_constraintStart_toStartOf="@+id/imageView7" />
    </ConstraintSet>
    <Transition
        app:constraintSetStart="@+id/spintop"
        app:constraintSetEnd="@+id/spinbottom"
        app:duration="601">
        <KeyFrameSet >
            <KeyAttribute
                app:motionTarget="@+id/imageView7"
                app:framePosition="50"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/imageView7"
                app:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/imageView7"
                app:framePosition="100"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/imageView7"
                app:framePosition="70"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/textView"
                app:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/textView"
                app:framePosition="100"
                android:alpha="0" />


        </KeyFrameSet>
        <OnSwipe app:maxAcceleration="7.69" />
        <OnClick app:clickAction="transitionToEnd" />
    </Transition>

    <Transition
        app:constraintSetStart="@+id/spinbottom"
        app:constraintSetEnd="@+id/spintop"
        app:motionInterpolator="cubic(0.6,1,0.4,0)">
        <KeyFrameSet >
            <KeyAttribute
                app:motionTarget="@+id/imageView7"
                app:framePosition="0"
                android:alpha="100" />
            <KeyAttribute
                app:motionTarget="@+id/textView"
                app:framePosition="0"
                android:alpha="0.5" />
            <KeyAttribute
                app:motionTarget="@+id/imageView7"
                app:framePosition="0"
                android:alpha="0.5" />



        </KeyFrameSet>
    </Transition>
    <Transition
        app:constraintSetStart="@+id/spinbottom"
        app:constraintSetEnd="@+id/finshSpin"
        app:duration="1082">
        <KeyFrameSet >
            <KeyAttribute
                app:motionTarget="@+id/imageView7"
                app:framePosition="0"
                android:alpha="100" />
            <KeyAttribute
                app:motionTarget="@+id/textView"
                app:framePosition="0"
                android:alpha="100" />
        </KeyFrameSet>
    </Transition>


</MotionScene>
